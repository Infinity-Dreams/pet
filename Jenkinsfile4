pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  
   stages {
    stage('DComposeUp') {
            steps {
                      sh 'docker-compose up -d'
            }
        }
    stage('HealthCheck') {
      steps {
             echo 'stage B started'
             sleep 300
             sh 'curl http://localhost:8181'
      }
   }
  }
   
}
