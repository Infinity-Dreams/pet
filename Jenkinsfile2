pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  
   stages {
    stage('Compile') {
       steps {
         sh 'mvn compile' //only compilation of the code
       }
    }   
    stage('Docker Build') {
      agent any
      steps {
        sh 'docker build -t st251/spring-petclinic:latest .'
      }
    }
  }
}
