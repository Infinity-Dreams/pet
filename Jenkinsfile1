pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
   stages {
    stage('SonarQScan') {
      steps {
       withSonarQubeEnv('SonarCloud') {
    sh "./mvnw org.jacoco:jacoco-maven-plugin:prepare-agent verify \
        sonar:sonar -Dsonar.branch.name=${env.BRANCH_NAME}"
          }
      }
    }
 
 
  }
   
}
